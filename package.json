{
  "name": "concyted-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:war": "ng build --configuration production --base-href /ctivitae/ && cd dist/concyted-frontend/browser && npx bestzip ../../../ctivitae.war *"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.8.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "apexcharts": "^5.3.6",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "ng-apexcharts": "^2.0.4",
    "ng-recaptcha": "^13.2.1",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.26.16",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.0",
    "bestzip": "^2.2.1",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
